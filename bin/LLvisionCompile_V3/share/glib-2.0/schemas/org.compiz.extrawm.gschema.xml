    <key type="s" name="toggle-redirect-key">
      <default>'Disabled'</default>
      <summary>Toggle Redirect</summary>
      <description>Toggle window redirect</description>
    </key>
    <key type="s" name="toggle-fullscreen-key">
      <default>'Disabled'</default>
