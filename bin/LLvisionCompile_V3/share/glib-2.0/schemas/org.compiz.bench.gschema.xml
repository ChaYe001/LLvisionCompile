<?xml version="1.0"?>
<schemalist>
  <schema id="org.compiz.bench" gettext-domain="compiz">
    <key type="s" name="initiate-key">
      <default>'&lt;Super&gt;F12'</default>
      <summary>Initiate</summary>
      <description>Start benchmark</description>
    </key>
    <key type="i" name="fps-limiter-mode">
      <default>0</default>
      <summary>FPS Limiter Mode</summary>
      <description>Integrated Compiz FPS limiter mode (0 = Limiter disabled, 1 = Default limiter, 2 = V-Sync-like limiter)</description>
    </key>
    <key type="b" name="output-screen">
      <default>true</default>
      <summary>Enable</summary>
      <description>Display FPS on screen</description>
    </key>
    <key type="i" name="position-x">
      <default>100</default>
      <summary>X position</summary>
      <description>X Position of benchmark window (0 - 4096)</description>
    </key>
    <key type="i" name="position-y">
      <default>50</default>
      <summary>Y position</summary>
      <description>Y Position of benchmark window (0 - 4096)</description>
    </key>
    <key type="b" name="output-console">
      <default>false</default>
      <summary>Enable</summary>
      <description>Print FPS to console</description>
    </key>
    <key type="i" name="console-update-time">
      <default>5</default>
      <summary>Update time</summary>
      <description>Console output update time (1 - 60)</description>
    </key>
  </schema>
</schemalist>
